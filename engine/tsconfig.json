{
  "include": ["./**/*.ts"],
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2023",
    "module": "es2022",
    "lib": ["es2023"],
    "moduleResolution": "bundler",
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
